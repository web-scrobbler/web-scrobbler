{
  "name": "web-scrobbler",
  "version": "3.0.0",
  "author": "David Å abata <posta@davidsabata.cz>",
  "contributors": [
    "@mountainash (https://mountainash.id.au/)"
  ],
  "description": "Scrobble music all around the web!",
  "private": true,
  "type": "module",
  "scripts": {
    "build:chrome": "cross-env BROWSER=chrome tsx build.ts",
    "build:firefox": "cross-env BROWSER=firefox tsx build.ts",
    "build:safari": "cross-env BROWSER=safari tsx build.ts",
    "buildts": "tsc --project tsconfig.connectors.json",
    "test": "vitest",
    "lint": "eslint . && stylelint \"**/*.scss\" && remark ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/web-scrobbler/web-scrobbler.git"
  },
  "bugs": {
    "url": "https://github.com/web-scrobbler/web-scrobbler/issues"
  },
  "homepage": "https://github.com/yayuyokitano/plugin-loader#readme",
  "dependencies": {
    "@suid/icons-material": "^0.5.9",
    "blueimp-md5": "^2.19.0",
    "metadata-filter": "^1.1.1",
    "stylelint": "^13.13.1",
    "vite": "^4.1.4",
    "webextension-polyfill": "^0.10.0"
  },
  "devDependencies": {
    "@types/blueimp-md5": "^2.18.0",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.14.6",
    "@types/react": "^18.0.28",
    "@types/webextension-polyfill": "^0.10.0",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.35.0",
    "eslint-config-web-scrobbler": "^2.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-solid": "^0.11.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "fs-extra": "^11.1.0",
    "prettier": "^2.8.4",
    "prettier-config-web-scrobbler": "^0.1.0",
    "remark": "^14.0.2",
    "remark-cli": "^11.0.0",
    "remark-preset-lint-recommended": "^6.1.2",
    "sass": "^1.58.3",
    "solid-js": "^1.6.13",
    "stylelint-config-web-scrobbler": "^0.2.0",
    "tsx": "^3.12.5",
    "typescript": "^4.9.5",
    "vite-plugin-solid": "^2.6.1",
    "vite-plugin-static-copy": "^0.13.1",
    "vitest": "^0.29.7"
  },
  "engines": {
    "node": ">=16.14.2"
  },
  "prettier": "prettier-config-web-scrobbler",
  "remark": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-web-scrobbler"
  },
