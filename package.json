{
  "name": "web-scrobbler",
  "version": "2.34.0",
  "author": "David Å abata <posta@davidsabata.cz>",
  "description": "Scrobble music all around the web!",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/web-scrobbler/web-scrobbler.git"
  },
  "dependencies": {
    "blueimp-md5": "2.16.0",
    "bootstrap": "5.0.0-alpha1",
    "bootstrap-icons": "1.0.0-alpha5",
    "metadata-filter": "0.4.1",
    "showdown": "1.9.1",
    "simple-icons": "2.18.0",
    "vue": "2.6.11",
    "webextension-polyfill": "0.6.0"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-spies": "1.0.0",
    "copy-webpack-plugin": "6.0.2",
    "css-loader": "3.6.0",
    "dotenv": "8.2.0",
    "eslint-config-web-scrobbler": "0.6.1",
    "eslint-plugin-vue": "7.0.0-alpha.6",
    "grunt": "1.2.1",
    "grunt-amo-upload": "0.3.0",
    "grunt-bump": "0.8.0",
    "grunt-cli": "1.3.2",
    "grunt-contrib-clean": "2.0.0",
    "grunt-contrib-compress": "1.6.0",
    "grunt-eslint": "23.0.0",
    "grunt-html-validate": "0.4.5",
    "grunt-jsonlint": "2.1.2",
    "grunt-lintspaces": "0.9.0",
    "grunt-mocha-cli": "6.0.0",
    "grunt-publish-github-drafts": "0.2.0",
    "grunt-remark": "9.0.0",
    "grunt-stylelint": "0.15.0",
    "grunt-webpack": "3.1.3",
    "grunt-webstore-upload": "0.9.24",
    "html-loader": "1.1.0",
    "html-validate": "2.21.0",
    "html-validate-vue": "3.1.0",
    "html-webpack-plugin": "4.3.0",
    "imagemin-webpack-plugin": "2.4.2",
    "jit-grunt": "0.10.0",
    "load-grunt-tasks": "5.1.0",
    "mini-css-extract-plugin": "0.9.0",
    "mocha": "8.0.1",
    "mocha-lcov-reporter": "1.3.0",
    "node-define": "0.1.1",
    "nyc": "15.1.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "preprocess-loader": "0.3.0",
    "prettier-config-web-scrobbler": "0.1.0",
    "remark-preset-lint-recommended": "4.0.1",
    "style-loader": "1.2.1",
    "stylelint": "13.6.1",
    "stylelint-config-web-scrobbler": "0.2.0",
    "svg-sprite-loader": "5.0.0",
    "vue-loader": "15.9.2",
    "vue-template-compiler": "2.6.11",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0",
    "write-file-webpack-plugin": "4.5.1"
  },
  "engines": {
    "node": ">=12.13.0"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "lint": "grunt lint",
    "test": "grunt test",
    "test-with-coverage": "nyc grunt test",
    "upload-coverage": "curl -Ls https://coverage.codacy.com/get.sh | bash -s report -r ./coverage/lcov.info",
    "clean-coverage": "rm -rf ./coverage"
  },
  "eslintConfig": {
    "extends": [
      "plugin:vue/base",
      "web-scrobbler"
    ],
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "strict": "off"
    },
    "plugins": [
      "vue"
    ]
  },
  "prettier": "prettier-config-web-scrobbler",
  "remark": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-web-scrobbler"
  },
  "license": "MIT"
}
