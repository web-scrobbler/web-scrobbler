    "imagemin": "8.0.1",
    "imagemin-jpegtran": "7.0.0",
    "imagemin-pngquant": "9.0.2",
    "prettier": "2.8.7",
    "prettier-config-web-scrobbler": "0.1.0",
    "remark": "14.0.2",
