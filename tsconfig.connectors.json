{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "module": "ES2020",
    "moduleResolution": "Node",
    "outDir": "build/connectorraw",
    "paths": {
      "@/*": ["src/*"],
      "#/*": ["tests/*"]
    },
    "resolveJsonModule": true,
    "sourceMap": true,
    "strict": true,
    "target": "ES2022"
  },
  "include": ["src/connectors"]
}
